package j77;

public class Employee extends Person{
	private int income;
	
	Employee(String name, String surename, int income) {
		super(name, surename);
		this.income = income;
	}
	

	public int getIncome() {
		return income;
	}

	public void setIncome(int income) {
		this.income = income;
	}
	
	public String getEmployee(){
		return getName() + " " + getSurename() + " " + getIncome();	
	}
	
}
