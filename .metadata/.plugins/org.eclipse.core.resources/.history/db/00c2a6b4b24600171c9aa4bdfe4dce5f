package jas4;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JTextField;

public class Text extends JFrame{
	JButton  b1;
	JLabel l1;
	JTextField t1;
	
	Text(){
		setSize(500, 500);
		setLayout(null);
		setVisible(true);
		
		l1 = new JLabel("Hello word");
		l1.setBounds(230, 100, 80, 30);
		this.add(l1);
		
		t1 = new JTextField("Hello word");
		t1.setBounds(50, 60, 60, 60);
		this.add(t1);
		
		b1 = new JButton("Ok");
		t1.setBounds(50, 60, 60, 60);
		this.add(t1);
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		new Text();
	}

}
